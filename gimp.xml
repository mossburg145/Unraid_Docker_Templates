<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>GIMP</Name>
  <Description>GIMP.&#13;
[br][br][b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]&#13;
[b]/config[/b] : Folder where GIMP's configuration is stored[br]&#13;
[b]8080[/b] : xRDP web-interface.[br]
[b]3389[/b] : xRDP client interface</Description>
  <Registry>http://localhost:5000/mossburg145/gimp</Registry>
  <Repository>mossburg145/gimp</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
	  <Port>
        <HostPort>3389</HostPort>
        <ContainerPort>3389</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/GIMP/data</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/appdata/GIMP/fonts</HostDir>
      <ContainerDir>/usr/share/fonts</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:[PORT:8080]</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
  <Overview>I</Overview>
  <Beta>False</Beta>
  <Category>Productivity:</Category>
  <Date>2016-02-11</Date> 
  <Support>http://lime-technology.com/forum/index.php?topic=46495.0</Support>
</Container>
