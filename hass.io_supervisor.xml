<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://github.com/mossburg145/unraid-docker-templates/blob/master/hass.io_supervisor.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>MediaApp:Video MediaApp:Music MediaApp:Photos MediaServer:Video MediaServer:Music MediaServer:Photos</Category>
  <Name>Hass.io</Name>
  <Description>First private cloud OS for home automation.</Description>
  <Overview>
    The Plex Media Server enriches your life by organizing all your personal media, presenting it beautifully and streaming it to all of your devices. It's easy to use, it's awesome, and it's free!
  </Overview>
  <Support>https://home-assistant.io/help/</Support>
  <Registry>https://registry.hub.docker.com/u/homeassistant/home-assistant/</Registry>
  <GitHub>https://github.com/binhex/arch-plex</GitHub>
  <Repository>homeassistant/amd64-hassio-supervisor</Repository>
  <Project>https://home-assistant.io</Project>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>host</Mode>
    <Publish>
    </Publish>
  </Networking>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/etc/locatetime</HostDir>
      <ContainerDir>/etc/locatetime</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/hassio</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/var/run/dbus</HostDir>
      <ContainerDir>/var/run/dbus</ContainerDir>
      <Mode>rw</Mode>
    <Volume>
      <HostDir>/var/run/docker.sock</HostDir>
      <ContainerDir>/var/run/docker.sock</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8123]</WebUI>
  <Icon>https://raw.githubusercontent.com/balloob/unraid-docker-templates/master/balloob/home-assistant-icon.png</Icon>
  </DonateText>
  </DonateLink>
  </DonateImg>
  <ExtraParams></ExtraParams>
</Containers>
