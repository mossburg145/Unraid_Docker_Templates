<?xml version="1.0"?>
<Container version="2">
  <Name>onedrive</Name>
  <Repository>patricol/onedrive:latest</Repository>
  <Registry>https://hub.docker.com/r/patricol/onedrive/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/Patricol/docker-templates/issues</Support>
  <Project>https://hub.docker.com/r/patricol/onedrive/</Project>
  <Overview/>
  <Category/>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/patricol/docker-templates/master/patricol/onedrive.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/patricol/docker-templates/master/patricol/images/onedrive.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled/>
  <DonateText/>
  <DonateLink/>
  <Description/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/sync/onedrive/</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/onedrive</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value></Value>
      <Name>URI_RESPONSE</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Host Path 2" Target="/data" Default="/mnt/user/onedrive/" Mode="rw" Description="Container Path: /data" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/sync/onedrive/</Config>
  <Config Name="Key 1" Target="URI_RESPONSE" Default="" Mode="" Description="Container Variable: URI_RESPONSE" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/onedrive" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/onedrive</Config>
</Container>